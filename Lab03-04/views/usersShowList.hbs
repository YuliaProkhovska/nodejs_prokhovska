<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
    <style>
        <!-- Стилі --> </style>
</head>
<body>
<h1>User List</h1>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th><th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {{#each users}}
        <tr>
            <td>{{this.nickname}}</td>
            <td>{{this.email}}</td>
            <td>{{this.age}}</td>
            <td>
                <form action="/user/delete/{{this._id}}" method="POST">
                    <button type="submit">Delete</button>
                </form>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
<a href="/auth/logout" class="logout-button">Logout</a>
</body>
</html>
